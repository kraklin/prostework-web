{
    "env": {
        "browser": true,
        "es5": true
    },
    "extends": ["airbnb", "prettier", "prettier/react"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ]
}

